{
"$schema": "https://vega.github.io/schema/vega-lite/v6.json",
"data": {
    "values": [
      {"numbers": {{include("bubbleCalc", numbers, steps)}} }
    ]
  },
  "transform": [
    {"flatten": ["numbers"]},
    {"window": [{
      "op": "rank",
      "as": "rank"
    }]}

  ],
  "mark": "bar",
  "encoding": {
    "x": {"field": "rank", "type": "nominal", "axis": {"labelAngle": 0}},
    "y": {"field": "numbers", "type": "quantitative"}
  }
}