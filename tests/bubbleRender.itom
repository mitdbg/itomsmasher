{
"data": {
    "values": [
      {"nums": {{numbers}}, 
      "rnk": [1, 2, 3, 4, 5]}
    ]
  },
  "transform": [
    {"flatten": ["nums", "rnk"]},
  ],
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "description": "A simple bar chart with embedded data.",
  "mark": "bar",
  "encoding": {
    "x": {"field": "rnk", "type": "nominal", "axis": {"labelAngle": 0}},
    "y": {"field": "nums", "type": "quantitative"}
  }
}